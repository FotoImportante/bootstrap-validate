{
  "name": "bootstrap-validate",
  "description": "A simple Form Validation Utility for Bootstrap not depending on jQuery",
  "version": "1.0.10",
  "main": "dist/bootstrap-validate.js",
  "repository": "git@github.com:PascaleBeier/bootstrap-validate.git",
  "author": "Pascale Beier <mail@pascalebeier.de>",
  "keywords": [
    "bootstrap",
    "validation",
    "form",
    "validate",
    "bootstrap3",
    "form-validation"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "webpack --config build/library.config.js -w",
    "build": "webpack --config build/library.config.js -p",
    "docs:dev": "webpack --config build/docs.config.js -w",
    "docs:build": "webpack --config build/docs.config.js -p",
    "test": "jest",
    "precommit": "lint-staged",
    "check-style": "eslint build/ src/ __tests__/",
    "fix-style": "eslint --fix build/ src/ __tests__/",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "build/*.js": [
      "prettier --write",
      "git add"
    ],
    "__tests__/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-env": "^1.5.2",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "classlist.js": "https://github.com/eligrey/classList.js#1fd9951666f940ac415dfe5dfa61cf902cc03b5e",
    "element-closest": "^2.0.2"
  }
}
