{
  "name": "bootstrap-validate",
  "description": "A simple Form Validation Utility for Bootstrap not depending on jQuery",
  "version": "1.0.11",
  "main": "dist/bootstrap-validate.js",
  "repository": "git@github.com:PascaleBeier/bootstrap-validate.git",
  "author": "Pascale Beier <mail@pascalebeier.de>",
  "keywords": [
    "bootstrap",
    "validation",
    "form",
    "validate",
    "bootstrap3",
    "form-validation"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "webpack --config build/library.config.js -w",
    "build": "webpack --config build/library.config.js -p",
    "docs:dev": "webpack --config build/docs.config.babel.js -w",
    "docs:build": "webpack --config build/docs.config.babel.js -p",
    "test": "jest",
    "precommit": "lint-staged",
    "check-style": "eslint build/ src/ __tests__/",
    "fix-style": "eslint --fix build/ src/ __tests__/",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "build/*.js": [
      "prettier --write",
      "git add"
    ],
    "__tests__/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-env": "^1.5.2",
    "bootstrap-loader": "^2.2.0",
    "bootstrap-sass": "^3.3.7",
    "bootstrap.native": "^2.0.14",
    "css-loader": "^0.28.7",
    "docblock-parser": "^1.0.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.1",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "jstransformer-markdown-it": "^2.0.0",
    "lint-staged": "^4.1.3",
    "node-sass": "^4.5.3",
    "prettier": "^1.6.1",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.4.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
